{
  "name": "mui-table-2025",
  "version": "1.0.0",
  "description": "Enhanced MUI DataTables with modern styling and improved UX - Security fixes and build improvements",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs"
    },
    "./components/*": {
      "import": "./src/components/*.js",
      "require": "./src/components/*.js"
    },
    "./utils": {
      "import": "./src/utils.js",
      "require": "./src/utils.js"
    },
    "./hooks/*": {
      "import": "./src/hooks/*.js",
      "require": "./src/hooks/*.js"
    },
    "./plug-ins/*": {
      "import": "./src/plug-ins/*.js",
      "require": "./src/plug-ins/*.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --progress --colors",
    "test": "cross-env NODE_ENV=test mocha --require @babel/register test/**/*.test.js",
    "docs:dev": "next docs",
    "docs:build": "cross-env NODE_ENV=production next build docs",
    "docs:export": "next export docs -o docs/export",
    "docs:deploy": "npm run docs:build && npm run docs:export && firebase deploy",
    "docs:start": "next start docs",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --require @babel/register test/*.js && nyc report --reporter=html | cat coverage/lcov.info | coveralls",
    "coverage:html": "cross-env NODE_ENV=test nyc check-coverage --lines 55 --reporter=html --reporter=text mocha --require @babel/register test/*.js && nyc report --reporter=html",
    "prettier": "find src/ docs/ test/ -type f -name \"*.js\" ! -path \"*/.next/*\" | xargs prettier --write",
    "lint": "eslint src",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/mui-datatable.git"
  },
  "keywords": [
    "mui",
    "material-ui",
    "datatables",
    "react",
    "table",
    "enhanced",
    "modern",
    "styling"
  ],
  "author": "Your Name",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/your-username/mui-datatable/issues"
  },
  "homepage": "https://github.com/your-username/mui-datatable#readme",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-external-helpers": "^7.25.9",
    "@babel/plugin-transform-async-to-generator": "^7.25.9",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-object-rest-spread": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/register": "^7.25.9",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@lopatnov/rollup-plugin-uglify": "^2.1.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^27.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-replace": "^5.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chai": "^4.3.0",
    "core-js": "^3.36.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.0.0",
    "ignore-styles": "^5.0.1",
    "jest": "^29.0.0",
    "jsdom": "^24.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^10.3.0",
    "next": "^15.4.7",
    "nyc": "^15.1.0",
    "prettier": "^3.2.0",
    "prismjs": "^1.29.0",
    "raw-loader": "^4.0.2",
    "react": "19.1.1",
    "react-dnd-test-backend": "^16.0.0",
    "react-dnd-test-utils": "^16.0.0",
    "react-dom": "19.1.1",
    "react-intersection-observer": "^9.8.0",
    "react-router-dom": "^6.22.0",
    "rollup": "^4.47.1",
    "rollup-pluginutils": "^2.8.2",
    "simulant": "^0.2.2",
    "sinon": "^17.0.0",
    "style-loader": "^3.3.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.26.0",
    "@emotion/cache": "^11.13.5",
    "clsx": "^2.1.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.2",
    "lodash.assignwith": "^4.2.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.find": "^4.6.0",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.isundefined": "^3.0.1",
    "lodash.memoize": "^4.1.2",
    "lodash.merge": "^4.6.2",
    "prop-types": "^15.8.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-sortable-tree-patch-react-17": "^2.9.0",
    "rollup-plugin-css-only": "^4.5.2",
    "run": "^1.5.0",
    "tss-react": "^4.9.15"
  },
  "side-effects": false,
  "nyc": {
    "lines": 50,
    "statements": 50,
    "functions": 50,
    "branches": 50,
    "check-coverage": true,
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "test/**/*.test.js"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
